   "source": [
    "import numpy as np\n",
    "\n",
    "data = np.array([5,10,11,12,15,35,50,55,72,92,204,215])\n",
    "\n",
    "bins = np.array_split(data, 3)\n",
    "\n",
    "smoothed = []\n",
    "for b in bins:\n",
    "    mean = b.mean()\n",
    "    smoothed.append(np.full(len(b), mean))\n",
    "\n",
    "print(\"Bins:\", bins)\n",
    "print(\"Smoothed:\", smoothed)\n"
   ]
  }
 ],
